<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="rootAnchorPane" prefHeight="100" prefWidth="300" styleClass="leftBar" stylesheets="/GUI/leftBar.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controll.DuelBarController">
   <VBox layoutX="10" layoutY="10" prefHeight="100.0" prefWidth="290.0">
      <children>
         <AnchorPane prefHeight="50.0" prefWidth="260.0">
            <children>
                <Label fx:id="player1Text" layoutY="5.0" prefHeight="25.0" prefWidth="75" text="Player 1:" />
                <Button fx:id="btnChoosePlayer1Dir" layoutX="63.0" layoutY="5.0" onAction="#btnChoosePlayer1DirPressed" prefHeight="25.0" prefWidth="216.0" text="choose directory" />
            </children>
            <padding>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
         </AnchorPane>
          <AnchorPane prefHeight="50.0">
              <children>
                  <Label fx:id="player2Text" layoutY="5.0" prefHeight="25.0" prefWidth="75" text="Player 2:" />
                  <Button fx:id="btnChoosePlayer2Dir" layoutX="63.0" layoutY="5.0" onAction="#btnChoosePlayer2DirPressed" prefHeight="25.0" prefWidth="216.0" text="choose directory" />
              </children>
          </AnchorPane>
      </children>
   </VBox>

</AnchorPane>
